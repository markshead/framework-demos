<html t:type="layout" title="Add Students And Courses"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

    <h2>Add Students and Courses</h2>
    <hr/>

    <h3>Add a Person:</h3>
    <t:beaneditform t:id="addStudent" t:object="student"/>

    <h3>Add a course:</h3>
    <t:beaneditform t:id="addCourse" t:object="course"/>

</html>
